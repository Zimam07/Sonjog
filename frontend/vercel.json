{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://sonjog.onrender.com/api/v1",
    "VITE_SOCKET_URL": "https://sonjog.onrender.com"
  }
}
